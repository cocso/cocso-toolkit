{
  "name": "@cocso-toolkit/react",
  "version": "0.0.1-beta.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cocso/cocso-toolkit.git",
    "directory": "packages/react"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rm -rf lib && bun run build.js && bun x tsc",
    "dev": "bun run build.js --watch",
    "clean": "rm -rf lib node_modules",
    "lint": "echo 'lint script not implemented'",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@cocso-toolkit/core": "workspace:*",
    "@internal/typescript-config": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "esbuild": "^0.25.6",
    "prettier": "^3.6.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": "^19 || ^18"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
